{
	"name": "quickmetrics-action",
	"version": "1.0.2",
	"description": "A GitHub action that send metrics to Quickmetrics",
	"main": "target/dist/index.js",
	"author": "Christian Rackerseder <git@echooff.de>",
	"license": "MIT",
	"scripts": {
		"compile": "tsc --build",
		"build": "ncc build target/src/index.js --out target/dist/ --minify",
		"lint": "eslint src test --ext '.ts'",
		"copy:paste:detector": "jscpd src test",
		"test:unit": "vitest",
		"test:unit:coverage": "vitest run --coverage"
	},
	"keywords": [],
	"dependencies": {
		"@actions/core": "1.10.0",
		"got": "11.8.5",
		"p-pipe": "3.1.0",
		"tslib": "2.4.0"
	},
	"devDependencies": {
		"@semantic-release/changelog": "6.0.1",
		"@semantic-release/commit-analyzer": "9.0.2",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "8.0.6",
		"@semantic-release/npm": "9.0.1",
		"@semantic-release/release-notes-generator": "10.0.3",
		"@types/sinon": "10.0.13",
		"@typescript-eslint/eslint-plugin": "5.40.0",
		"@typescript-eslint/parser": "5.40.0",
		"@vercel/ncc": "0.34.0",
		"@vitest/coverage-c8": "0.24.1",
		"c8": "7.12.0",
		"eslint": "8.25.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.2.1",
		"jscpd": "3.5.0",
		"prettier": "2.7.1",
		"semantic-release": "19.0.5",
		"sinon": "14.0.1",
		"typescript": "4.8.4",
		"vitest": "0.24.1"
	}
}
