{
  "name": "quickmetrics-action",
  "version": "0.1.0",
  "description": "A GitHub action that send metrics to Quickmetrics",
  "main": "target/dist/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build",
    "build": "ncc build target/src/index.js --out target/dist/ --minify",
    "lint": "eslint src test --ext '.ts'",
    "copy:paste:detector": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json c8 mocha"
  },
  "keywords": [],
  "dependencies": {
    "@actions/core": "1.2.7",
    "got": "11.8.2",
    "p-pipe": "3.1.0",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "github:semantic-release/github",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/chai": "4.2.17",
    "@types/mocha": "8.2.2",
    "@types/sinon": "10.0.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "@vercel/ncc": "0.28.5",
    "@zeit/ncc": "0.22.3",
    "c8": "7.7.2",
    "chai": "4.3.4",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "jscpd": "3.3.25",
    "mocha": "8.3.2",
    "prettier": "2.2.1",
    "semantic-release": "17.4.2",
    "sinon": "10.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
