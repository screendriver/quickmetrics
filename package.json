{
  "name": "quickmetrics-action",
  "version": "0.1.0",
  "description": "A GitHub action that send metrics to Quickmetrics",
  "main": "target/dist/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint --ext ts,tsx src test",
    "copy:paste:detector": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json nyc mocha",
    "build": "ncc build target/src/index.js --out target/dist/ --minify"
  },
  "keywords": [],
  "dependencies": {
    "@actions/core": "1.2.5",
    "got": "11.5.2",
    "p-pipe": "3.1.0",
    "tslib": "2.0.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/mocha": "8.0.3",
    "@types/sinon": "9.0.5",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "@zeit/ncc": "0.22.3",
    "chai": "4.2.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "jscpd": "3.3.19",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "sinon": "9.0.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  }
}
