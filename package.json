{
  "name": "quickmetrics-action",
  "version": "0.1.0",
  "description": "A GitHub action that send metrics to Quickmetrics",
  "main": "target/dist/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint --ext ts,tsx src test",
    "copy:paste:detector": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json nyc mocha",
    "build": "ncc build target/src/index.js --out target/dist/ --minify"
  },
  "keywords": [],
  "dependencies": {
    "@actions/core": "1.2.4",
    "got": "11.3.0",
    "p-pipe": "3.1.0",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/sinon": "9.0.4",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "@zeit/ncc": "0.22.3",
    "chai": "4.2.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mocha": "7.0.1",
    "eslint-plugin-prettier": "3.1.3",
    "jscpd": "3.3.0-rc.8",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "sinon": "9.0.2",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  }
}
