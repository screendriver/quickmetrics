{
  "name": "quickmetrics-action",
  "version": "0.1.0",
  "description": "A GitHub action that send metrics to Quickmetrics",
  "main": "target/dist/index.js",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build",
    "build": "ncc build target/src/index.js --out target/dist/ --minify",
    "lint": "eslint src test --ext '.ts'",
    "copy:paste:detector": "jscpd src test",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json c8 mocha"
  },
  "keywords": [],
  "dependencies": {
    "@actions/core": "1.2.6",
    "got": "11.8.0",
    "p-pipe": "3.1.0",
    "tslib": "2.0.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "github:semantic-release/github",
    "@semantic-release/npm": "7.0.8",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/chai": "4.2.14",
    "@types/mocha": "8.0.4",
    "@types/sinon": "9.0.8",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "@vercel/ncc": "0.25.1",
    "@zeit/ncc": "0.22.3",
    "c8": "7.3.5",
    "chai": "4.2.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "jscpd": "3.3.21",
    "mocha": "8.2.1",
    "prettier": "2.2.0",
    "semantic-release": "17.2.4",
    "sinon": "9.2.1",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
